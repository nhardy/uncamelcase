{
  "name": "uncamelcase",
  "version": "0.0.0",
  "description": "A utility module for converting camel case to a space-separated string",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "clean": "GLOBIGNORE=.gitignore rm -rf ./dist/*",
    "cover": "babel-node ./node_modules/.bin/isparta cover _mocha -- --reporter dot",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "postversion": "git push --follow-tags",
    "preversion": "npm run lint && npm run clean && npm run build",
    "start": "npm run clean && npm run build",
    "test": "mocha",
    "watch": "npm run clean && webpack --watch"
  },
  "keywords": [
    "uncamelcase",
    "camelcase",
    "space-separated"
  ],
  "author": "Nathan Hardy",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-nhardy": "^0.0.8",
    "eslint-plugin-mocha": "^2.2.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "lodash": "^4.11.1"
  }
}
